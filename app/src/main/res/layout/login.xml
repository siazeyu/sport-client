<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <Button
            android:text="返回登录"
            android:layout_width="199dp"
            android:layout_height="0dp" android:id="@+id/btn_ret"
            app:strokeColor="#2196F3" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="93dp" app:layout_constraintTop_toBottomOf="@+id/btn_reg"
            android:layout_marginBottom="147dp"/>
    <Button
            android:text="注册"
            android:layout_width="0dp"
            android:layout_height="0dp" android:id="@+id/btn_reg"
