            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword5" android:layout_marginBottom="36dp"
            app:layout_constraintEnd_toEndOf="@+id/btn_ret" app:layout_constraintBottom_toTopOf="@+id/btn_ret"
            app:layout_constraintStart_toStartOf="@+id/btn_ret"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editTextTextPassword5"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword4"
            android:layout_marginBottom="32dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/btn_reg" app:layout_constraintStart_toStartOf="parent"
